--Properties:

RemoteKeypad.Name = "RemoteKeypad"
RemoteKeypad.Parent = game.CoreGui
RemoteKeypad.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

KeypadContainer.Name = "KeypadContainer"
KeypadContainer.Parent = RemoteKeypad
KeypadContainer.Active = true
KeypadContainer.BackgroundColor3 = Color3.fromRGB(91, 91, 91)
KeypadContainer.BorderColor3 = Color3.fromRGB(76, 76, 76)
KeypadContainer.BorderSizePixel = 3
KeypadContainer.ClipsDescendants = true
KeypadContainer.Position = UDim2.new(0.445746005, 0, 0.357142866, 0)
KeypadContainer.Size = UDim2.new(0, 176, 0, 243)
KeypadContainer.Draggable = true

_0.Name = "0"
_0.Parent = KeypadContainer
_0.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
_0.BorderColor3 = Color3.fromRGB(211, 211, 211)
_0.BorderSizePixel = 2
_0.Position = UDim2.new(0.414772719, 0, 0.781893015, 0)
_0.Size = UDim2.new(0, 30, 0, 32)
_0.Font = Enum.Font.SourceSans
_0.Text = "0"
_0.TextColor3 = Color3.fromRGB(0, 0, 0)
_0.TextSize = 30.000
_0.MouseButton1Click:Connect(function()
	fireclickdetector(__0)
	
	if Result.Text == "" then
		Result.Text = Result.Text
	else
		Result.Text = Result.Text .. _0.Text
	end
end)

