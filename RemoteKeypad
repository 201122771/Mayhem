Result.Name = "Result"
Result.Parent = KeypadContainer
Result.BackgroundColor3 = Color3.fromRGB(132, 132, 132)
Result.BorderColor3 = Color3.fromRGB(115, 115, 115)
Result.BorderSizePixel = 2
Result.Position = UDim2.new(0.1875, 0, 0.119341567, 0)
Result.Size = UDim2.new(0, 110, 0, 21)
Result.Font = Enum.Font.SourceSans
Result.Text = ""
Result.TextColor3 = Color3.fromRGB(255, 255, 255)
Result.TextScaled = true
Result.TextSize = 25.000
Result.TextStrokeTransparency = 0.700
Result.TextWrapped = true
Result.Changed:Connect(function()
	Result.Text = Result.Text:sub(1,4)
end)

game:GetService("RunService").RenderStepped:Connect(function()
	if string.len(Result.Text) == 4 then
		wait(.2)
		Result.Text = ""
	end
end)


Exit.Name = "Exit"
Exit.Parent = KeypadContainer
Exit.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Exit.BackgroundTransparency = 1.000
Exit.Position = UDim2.new(0.863636374, 0, 0, 0)
Exit.Size = UDim2.new(0, 24, 0, 22)
Exit.Font = Enum.Font.SourceSans
Exit.Text = "X"
Exit.TextColor3 = Color3.fromRGB(255, 255, 255)
Exit.TextScaled = true
Exit.TextSize = 14.000
Exit.TextWrapped = true
Exit.MouseButton1Click:Connect(function()
	KeypadContainer:TweenSize(UDim2.new(0, 176,0, 0), "InOut", "Linear", 0.2)
	wait(.2)
	KeypadContainer.Visible = false
end)
